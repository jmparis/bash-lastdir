help:
	@echo ""
	@echo "Targets available"
	@echo ""
	@echo "make help		This help."
	@echo ""
	@echo "make install		Install the lastdir functionnality in your Bash environment."
	@echo "make uninstall		Remove the lastdir functionnality from your Bash configuration files"
	@echo ""

install:
	@echo "install lastdir"

uninstall:
	@echo "uninstall lastdir functionnality"